import turtle
bob=turtle.Turtle()
bob.speed(0)
def drawCircle(size):
  for i in range(36):
    bob.forward(size)
    bob.right(10)
bob.penup()
bob.goto(10,30)
bob.pendown()
drawCircle(30)
bob.forward(10)
bob.penup()
bob.goto(10,255)
bob.pendown()
bob.left(10)
drawCircle(20)
bob.penup()
bob.goto(10,420)
bob.pendown()
bob.left(10)
drawCircle(15)
bob.penup()
bob.goto(10,400)
bob.right(100)
bob.pendown()
bob.forward(40)
bob.penup()
bob.goto(67,400)
bob.pendown()
bob.forward(40)
