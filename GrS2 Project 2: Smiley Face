import turtle
bob=turtle.Turtle()
bob.speed(6)
screen=turtle.Screen()
screen.bgcolor("yellow")
bob.penup()
bob.goto(100,100)
bob.pendown()
bob.forward(20)
bob.right(90)
bob.forward(150)
bob.right(90)
bob.forward(20)
bob.right(90)
bob.forward(150)
bob.penup()
bob.goto(10,100)
bob.right(180)
bob.pendown()
for i in range(2):
  bob.forward(150)
  bob.right(90)
  bob.forward(20)
  bob.right(90)
bob.penup()
bob.goto(-100,-30)
bob.pendown()
for i in range(19):
  bob.forward(26)
  bob.left(10)
