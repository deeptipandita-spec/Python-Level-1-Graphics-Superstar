import turtle
bob=turtle.Turtle()
import random
size=random.randint(10,200)
x=random.randint(-360,360)
y=random.randint(-200,200)
r=random.randint(0,255)
b=random.randint(0,255)
g=random.randint(0,255)
bob.penup()
bob.color(r,g,b)
bob.goto(x,y)
bob.pendown()
bob.begin_fill()
for i in range(4):
  bob.forward(size)
  bob.right(90)
bob.end_fill()
