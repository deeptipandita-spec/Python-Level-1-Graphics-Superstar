import turtle
bob=turtle.Turtle()
bob.color("sky blue")
bob.begin_fill()
bob.forward(100)
bob.left(90)
bob.forward(100)
bob.left(90)
bob.forward(100)
bob.left(90)
bob.forward(100)
bob.end_fill()
bob.penup()
bob.goto(-100,-100)
bob.pendown()
bob.right(120)
bob.forward(60)
bob.right(120)
bob.forward(60)
bob.right(120)
bob.forward(60)
