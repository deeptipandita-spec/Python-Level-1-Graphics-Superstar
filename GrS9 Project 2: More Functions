import turtle
bob=turtle.Turtle()
bob.speed(0)
def drawTriangle(sideLength):
  for i in range(3):
    bob.forward(sideLength)
    bob.right(120)
drawTriangle(500)
def drawSquare(sideLength):
  for i in range(4):
    bob.forward(sideLength)
    bob.right(90)
drawSquare(20)
sideLength = 20
for i in range(10):
  drawSquare(sideLength)
  sideLength=sideLength+5
